printf :: fn(fmt: string, args: ..) #foreign;
scanf :: fn(fmt: string, args: ..) #foreign;

main :: fn() int {
    x := 20;

    printf("Pick a number: \n");
    scanf("%d", @x);

    printf("Counting up to %d\n", x);

    i := 1;
    while i <= x {
        printf("%d, ", i);
        if i % 10 == 0 {
            printf("\n");
        }
        i += 1;
    }

    return 0;
}
